import java.util.Scanner;

public class Operacion {
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner mio =new Scanner(System.in);
		
int num1=mio.nextInt();
int num2=mio.nextInt();

System.out.print("Ingrese numero 1"+num1);
System.out.print("Ingrese numero 1"+num2);


System.out.print("La suma es: ");
System.out.println(num1+num2);

System.out.print("La resta es: ");
System.out.println(num1-num2);

System.out.print("La multiplicacion es: ");
System.out.println(num1*num2);

System.out.print("La division es: ");
System.out.println(num1/num2);


	}

}
